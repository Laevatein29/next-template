{
  "name": "react-vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "mock": "node --no-warnings=ExperimentalWarning --loader ts-node/esm ./src/scripts/mock/index.mts",
    "generate": "node ./node_modules/intermock/build/src/cli/index.js --files ./src/types/index.ts --interfaces 'App'",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@generouted/react-router": "^1.19.3",
    "fast-glob": "^3.3.2",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "minimatch": "^9.0.4",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-color-palette": "^7.1.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.2",
    "react-router": "^6.23.0",
    "react-router-dom": "^6.23.0",
    "sass": "^1.76.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.1",
    "@chromatic-com/storybook": "^1.5.0",
    "@eslint-react/eslint-plugin": "^1.5.14",
    "@storybook/addon-essentials": "^8.1.5",
    "@storybook/addon-interactions": "^8.1.5",
    "@storybook/addon-links": "^8.1.5",
    "@storybook/addon-onboarding": "^8.1.5",
    "@storybook/blocks": "^8.1.5",
    "@storybook/react": "^8.1.5",
    "@storybook/react-vite": "^8.1.5",
    "@storybook/test": "^8.1.5",
    "@types/node": "^20.12.11",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "intermock": "^0.2.5",
    "storybook": "^8.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^5.2.0"
  }
